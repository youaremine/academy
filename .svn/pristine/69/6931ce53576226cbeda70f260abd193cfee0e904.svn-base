<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>Login</title>
<script language="javascript" type="text/javascript">
function OnUserNameFocus()
{
	document.getElementById('newPassword').focus();
}
</script>
</head>

<body onload="OnUserNameFocus();">
	<table width="100%" height="400" border="0" cellpadding="0"
		cellspacing="0" bgcolor="#E8F3FF">
		<tr>
			<td><form id="form1" name="form1" method="post" action="">
					<table width="400" border="0" align="center" cellpadding="0"
						cellspacing="0">
						<tr>
							<td colspan="2" style="color: #FF0000">{Error}</td>
						</tr>
						<tr>
							<td height="30">Surveyor Code:</td>
							<td><label> {surveyorCode}</label></td>
						</tr>
						<tr style="display: none;">
							<td height="30">Old Password:</td>
							<td><input name="oldPassword" type="password"
								id="oldPassword" /></td>
						</tr>
						<tr>
							<td height="30">New Password:</td>
							<td><input name="newPassword" type="password"
								id="newPassword" /></td>
						</tr>
						<tr>
							<td height="30">Confirm Password:</td>
							<td><input name="confirmPassword" type="password"
								id="confirmPassword" /></td>
						</tr>
						<tr>
							<td height="30">&nbsp;</td>
							<td><input name="Update" type="submit" id="Update"
								value="Update" /> <a href="surveyor_profile.php">Cancel</a></td>
						</tr>
					</table>
				</form></td>
		</tr>
	</table>
</body>
</html>
